node {
    background-color: teal;
    label: data(label);
    shape: roundrectangle;
    width: 40;
    height: 40;
}
node.has-been-selected {
    background-color: black;
}
node:selected {
    background-color: red;
}
edge {
    width: 4;
    line-color: tan;
    target-arrow-color: tan;
    target-arrow-shape: triangle;
    curve-style: bezier;
}
edge:selected {
    line-color: red;
    target-arrow-color: red;
}